{
  "name": "@davidpricedev/awsum",
  "description": "platform engineering cli",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davidpricedev/awsum.git"
  },
  "version": "0.1.0",
  "author": "davidpricedev",
  "bin": {
    "awsum": "./bin/run.js"
  },
  "bugs": "",
  "dependencies": {
    "@aws-sdk/client-ec2": "^3.654.0",
    "@aws-sdk/client-ecs": "^3.654.0",
    "@aws-sdk/client-lambda": "^3.665.0",
    "@aws-sdk/client-opensearch": "^3.654.0",
    "@aws-sdk/client-rds": "^3.654.0",
    "@aws-sdk/client-ssm": "^3.654.0",
    "@aws-sdk/client-sso": "^3.654.0",
    "@aws-sdk/client-sso-oidc": "^3.654.0",
    "@aws-sdk/client-sts": "^3.654.0",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "cli-table": "^0.3.11",
    "enquirer": "^2.4.1",
    "figlet": "^1.7.0",
    "json-chalkify": "^1.0.2",
    "ora": "^8.1.0"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/cli-table": "^0.3.4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "mocha": "^10",
    "oclif": "^4",
    "prettier": "^3.3.3",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "awsum",
    "dirname": "awsum",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " "
  },
  "pkg": {
    "scripts": "./dist/**/*.js"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "format": "prettier --write .",
    "install-precommit": "pre-commit install",
    "precommit": "pre-commit run  --all-files",
    "clean": "rm -rf dist && rm -rf node_modules && rm -rf tmp"
  },
  "types": "dist/index.d.ts"
}
